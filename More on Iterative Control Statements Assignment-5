1. Write a program to print MySirG N times on the screen.
#include<stdio.h>
int main()
{
    int i = 0,N;
    printf("How many times you want to print iNeuron on the screen: ");
    scanf("%d",&N);
    while (i<N)
    {
        printf("iNeuron\n");
        i++;
    }
    return 0;       
}   

2. Write a program to print the first N natural numbers.
#include<stdio.h>
int main()
{
    int i = 1,N;
    printf("Enter the number of natural numbers:");
    scanf("%d",&N);
    while (i<=N)
    {
        printf("%d\n",i);
        i++;
    }
    return 0;       
}   

3. Write a program to print the first N natural numbers in reverse order.
#include<stdio.h>
int main()
{
    int i;
    printf("Enter the Number:");
    scanf("%d",&i);
    while (i>0)
    {
        printf("%d\n",i);
        i--;
    }
    return 0;       
}   

4. Write a program to print the first N odd natural numbers.
#include<stdio.h>
int main()
{
    int N,i = 1;
    printf("Enter the Number: ");
    scanf("%d",&N);
    while (i<=N)
    {
        printf("%d\n",2*i-1);
        i++;
    }
    return 0;       
}   

5. Write a program to print the first N odd natural numbers in reverse order.
#include<stdio.h>
int main()
{
    int i;
    printf("Enter the Number: ");
    scanf("%d",&i);
    while (i>0)
    {
        printf("%d\n",2*i-1);
        i--;
    }
    return 0;       
}   

6. Write a program to print the first N even natural numbers.
#include<stdio.h>
int main()
{
    int i = 1,N;
    printf("Enter the Number: ");
    scanf("%d",&N);
    while (i<=N)
    {
        printf("%d\n",2*i);
        i++;
    }
    return 0;       
}   

7. Write a program to print the first N even natural numbers in reverse order.
#include<stdio.h>
int main()
{
    int i;
    printf("Enter the Number: ");
    scanf("%d",&i);
    while (i>0)
    {
        printf("%d\n",2*i);
        i--;
    }
    return 0;       
}   

8. Write a program to print squares of the first N natural numbers.
#include<stdio.h>
int main()
{
    int i = 1,N;
    printf("Enter the Number: ");
    scanf("%d",&N);
    while (i<=N)
    {
        printf("%d\n",i*i);
        i++;
    }
    return 0;       
}   

9. Write a program to print cubes of the first N natural numbers.
#include<stdio.h>
int main()
{
    int i = 1,N;
    printf("Enter the Number: ");
    scanf("%d",&N);
    while (i<=N)
    {
        printf("%d\n",i*i*i);
        i++;
    }
    return 0;       
}   

10. Write a program to print a table of N.
#include<stdio.h>
int main()
{
    int i = 1,N;
    printf("Enter the Number Of Table: ");
    scanf("%d",&N);
    while (i<=10)
    {
        printf("%d\n",i*N);
        i++;
    }
    return 0;       
}   
